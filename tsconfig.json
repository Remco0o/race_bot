{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "moduleResolution": "node",
    "lib": [
      "DOM",
      "ES2020"
    ],
    "paths": {
      "@assets/*": [
        "./assets/js/*",
      ],
      "@deps/*": [
        "./deps/*"
      ],
    },
    "strictNullChecks": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}